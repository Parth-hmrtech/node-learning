<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>EJS Users List</title>
  <link rel="stylesheet" href="/style.css"> <!-- Link to the CSS file -->
</head>
<body>
  <h1>Users Information</h1>
  
  <!-- Table to display multiple users -->
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th>
      </tr>
    </thead>
    <tbody>
      <!-- Loop through users array and display each user -->
      <% users.forEach(user => { %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.age %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

</body>
</html>
